    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Usuario</th>
            <td>{{ entity.usuario }}</td>
        </tr>
        <tr>
            <th>Activo</th>
            <td>{{ entity.activo }}</td>
