{% spaceless %}
<div class="modal fade"id="modal-solicitudes" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                <h4 class="modal-title">Solicitud</h4>
            </div>
            <div class="modal-body">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3 class="panel-title"><i><span class="glyphicon glyphicon-list-alt"></span></i> Datos de Solicitud</h3>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped table-hover ">
                            <tbody>
                                <tr>
                                    <th>Id</th>
                                    <td>{{ entity.id }}</td>
                                </tr>
                                <tr>
                                    <th>Fechadesde</th>
                                    <td>{{ entity.fechaDesde|date('Y-m-d H:i:s') }}</td>
                                </tr>
                                <tr>
                                    <th>Fechahasta</th>
                                    <td>{{ entity.fechaHasta|date('Y-m-d H:i:s') }}</td>
                                </tr>
                                <tr>
                                    <th>Perfilpostulante</th>
                                    <td>{{ entity.perfilPostulante }}</td>
                                </tr>
                                <tr>
                                    <th>Conocrequeridos</th>
                                    <td>{{ entity.conocRequeridos }}</td>
                                </tr>
                                <tr>
                                    <th>Nivelcarreracandidato</th>
                                    <td>{{ entity.nivelCarreraCandidato }}</td>
                                </tr>
                                <tr>
                                    <th>Solicitudatendida</th>
                                    <td>{{ entity.solicitudAtendida }}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">                        
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>
{% endspaceless %}