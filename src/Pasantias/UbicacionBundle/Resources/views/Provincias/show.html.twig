{% extends 'ThemeBundle:Default:base.html.twig' %}
{% block contenido %}
<h1>Provincias</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombprovincia</th>
            <td>{{ entity.nombreProvincia }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('pasantias_provincias') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('pasantias_provincias_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('pasantias_provincias_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{%endblock%}