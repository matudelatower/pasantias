{% extends 'ThemeBundle:Default:base.html.twig' %}
{% block contenido %}
<h1>Localidades edit</h1>

<form action="{{ path('pasantias_localidades_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('pasantias_localidades') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('pasantias_localidades_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{%endblock%}