{% extends 'ThemeBundle:Default:base.html.twig' %}
{% block contenido %}
<h1>Alta de Localidades</h1>

<div class="well bs-component col-lg-10">
    <form class="form-horizontal" action="{{ path('pasantias_localidades_create') }}" method="post" {{ form_enctype(form) }}>
        <fieldset>    
{{ form_widget(form) }}
        </fieldset>
        <div class="record_actions pull-right">
            <button class="btn btn-primary" type="submit"><i class="glyphicon glyphicon-floppy-disk"></i> Guardar</button>
            <a class="btn btn-default" href="{{ path('pasantias_localidades') }}">
                Volver
            </a>

        </div>

    </form>
</div>
{%endblock%}