<h1>Usuarios list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Usuario</th>
            <th>Clave</th>
            <th>Activo</th>
            <th>Telefono</th>
            <th>Correo</th>
            <th>Idperfil</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('pasantias_usuarios_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombre }}</td>
            <td>{{ entity.apellido }}</td>
            <td>{{ entity.usuario }}</td>
            <td>{{ entity.clave }}</td>
            <td>{{ entity.activo }}</td>
            <td>{{ entity.telefono }}</td>
            <td>{{ entity.correo }}</td>
            <td>{{ entity.idPerfil }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('pasantias_usuarios_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('pasantias_usuarios_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('pasantias_usuarios_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
