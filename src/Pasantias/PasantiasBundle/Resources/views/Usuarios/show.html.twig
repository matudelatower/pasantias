</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('pasantias_usuarios') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('pasantias_usuarios_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('pasantias_usuarios_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
